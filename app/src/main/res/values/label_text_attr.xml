<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LabelView">
        <attr name="rectColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="float"/>
        <attr name="rectHeight" format="dimension"/>
        <attr name="rectWidth" format="dimension"/>
        <attr name="text" format="string"/>
        <attr name="topLeftCornerRadius" format="float"/>
        <attr name="topRightCornerRadius" format="float"/>
        <attr name="bottomRightCornerRadius" format="float"/>
        <attr name="bottomLeftCornerRadius" format="float"/>
        <attr name="orientation">
            <enum name="vertical" value="0"/>
            <enum name="horizontal" value="1"/>
        </attr>
    </declare-styleable>

</resources>